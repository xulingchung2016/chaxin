<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:su="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#80000000"
     >
    
    <LinearLayout 
        android:id="@+id/rl_wheel"
         android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@color/bg_color"
        android:orientation="vertical"
        >
        <RelativeLayout 
            android:layout_width="fill_parent"
        android:layout_height="wrap_content"
            
            >
         <TextView
                    android:id="@+id/tv_quxiao"
                    style="@style/ItemSmallBlackText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/left_h_size"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/bg_white_grey"
                    android:layout_marginBottom="5dip"
                    android:text="取消" />
         <TextView
                    android:id="@+id/tv_queren"
                    style="@style/ItemSmallBlackText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:textSize="@dimen/left_h_size"
                    android:layout_marginRight="10dp"
                      android:background="@drawable/bg_white_grey"
                    android:layout_marginBottom="5dip"
                    android:layout_marginTop="10dp"
                    android:text="确认" />
         </RelativeLayout>
     <LinearLayout
         android:id="@+id/ll_3"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:visibility="gone"
        android:orientation="horizontal" >

         <com.newbrain.customview.ScrollerNumberPicker
        android:id="@+id/ww_1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        su:itemNumber="5"
        su:lineColor="@color/grey"
        su:maskHight="32dp"
        su:noEmpty="true"
       
        su:normalTextColor="#777"
        su:normalTextSize="@dimen/left_l_size"
        su:selecredTextColor="#ff000000"
        su:selecredTextSize="@dimen/text_m_size"
        su:unitHight="50dp" />

        <com.newbrain.customview.ScrollerNumberPicker
            android:id="@+id/ww_2"
            android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        su:itemNumber="5"
        su:lineColor="@color/grey"
        su:maskHight="32dp"
        su:noEmpty="true"
        su:normalTextColor="#777"
        su:normalTextSize="@dimen/left_l_size"
        su:selecredTextColor="#ff000000"
        su:selecredTextSize="@dimen/text_m_size"
        su:unitHight="50dp"  >
        </com.newbrain.customview.ScrollerNumberPicker>

        <com.newbrain.customview.ScrollerNumberPicker
            android:id="@+id/ww_3"
            android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="1dp"
        android:layout_weight="1"
        su:itemNumber="5"
        su:lineColor="@color/grey"
        su:maskHight="32dp"
        su:noEmpty="true"
        su:normalTextColor="#777"
        su:normalTextSize="@dimen/left_l_size"
        su:selecredTextColor="#ff000000"
        su:selecredTextSize="@dimen/text_m_size"
        su:unitHight="50dp" >
        </com.newbrain.customview.ScrollerNumberPicker>
    </LinearLayout>
     <LinearLayout
         android:id="@+id/ll_2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
         android:visibility="gone"
        android:orientation="horizontal" >

         <com.newbrain.customview.ScrollerNumberPicker
        android:id="@+id/ww_4"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        su:itemNumber="5"
        su:lineColor="@color/grey"
        su:maskHight="32dp"
        su:noEmpty="true"
       
        su:normalTextColor="#777"
        su:normalTextSize="@dimen/left_l_size"
        su:selecredTextColor="#ff000000"
        su:selecredTextSize="@dimen/left_h_size"
        su:unitHight="50dp" />

        <com.newbrain.customview.ScrollerNumberPicker
            android:id="@+id/ww_5"
            android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        su:itemNumber="5"
        su:lineColor="@color/grey"
        su:maskHight="32dp"
        su:noEmpty="true"
        su:normalTextColor="#777"
        su:normalTextSize="@dimen/left_l_size"
        su:selecredTextColor="#ff000000"
        su:selecredTextSize="@dimen/left_h_size"
        su:unitHight="50dp"  >
        </com.newbrain.customview.ScrollerNumberPicker>

    </LinearLayout>
    </LinearLayout>
    

</RelativeLayout>
